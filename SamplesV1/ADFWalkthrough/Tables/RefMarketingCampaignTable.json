{
  "name": "RefMarketingCampaignTable",
  "properties": {
    "structure": [
      {
        "name": "ID",
        "type": "String"
      },
      {
        "name": "Day",
        "type": "String"
      },
      {
        "name": "City",
        "type": "String"
      },
      {
        "name": "State",
        "type": "String"
      },
      {
        "name": "Type",
        "type": "String"
      },
      {
        "name": "Impressions",
        "type": "Int"
      }
    ],
    "type": "AzureBlob",
    "linkedServiceName": "StorageLinkedService",
