{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "datawarehouse": { "type": "string" }
    },
    "variables": {},
    "resources": [
        {
            "name": "Copy data from sample data (Wide World Importers) to the Warehouse table",
            "description": "Use this template to copy data from sample data (Retail Data Model from Wide World Importers) to a specified table in Warehouse.",
            "type": "pipelines",
            "apiVersion": "2018-06-01",
            "properties": {
                "activities": [
                    {
                        "name": "Copy from sample data to DW",
                        "description": "Use this template to copy data from sample data (Retail Data Model from Wide World Importers) to a specified table in Warehouse.",
                        "type": "Copy",
                        "dependsOn": [],
                        "policy": {
                            "timeout": "0.12:00:00",
                            "retry": 0,
                            "retryIntervalInSeconds": 30,
                            "secureOutput": false,
                            "secureInput": false
                        },
                        "typeProperties": {
                            "source": {
                                "type": "ParquetSource",
                                "storeSettings": {
                                    "type": "AzureBlobStorageReadSettings",
                                    "recursive": true,
                                    "wildcardFolderPath": "WideWorldImportersDW/parquet/full/dimension_city",
                                    "wildcardFileName": "*.parquet"
                                },
                                "formatSettings": { "type": "ParquetReadSettings" },
                                "datasetSettings": {
                                    "annotations": [],
                                    "linkedService": {
                                        "name": "dimension_city",
                                        "properties": {
                                            "type": "AzureBlobStorage",
                                            "typeProperties": {
                                                "containerUri": "https://azuresynapsestorage.blob.core.windows.net/sampledata",
                                                "authenticationType": "Anonymous"
                                            }
                                        }
                                    },
                                    "type": "Parquet",
                                    "typeProperties": {
                                        "location": {
                                            "type": "AzureBlobStorageLocation",
                                            "container": "sampledata"
                                        },
                                        "compressionCodec": "snappy"
                                    }
                                }
                            },
                            "sink": {
                                "type": "DataWarehouseSink",
                                "allowCopyCommand": true,
                                "copyCommandSettings": {},
                                "tableOption": "autoCreate",
                                "datasetSettings": {
                                    "annotations": [],
                                    "type": "DataWarehouseTable",
                                    "schema": [],
                                    "typeProperties": {
                                        "schema": "dbo",
                                        "table": "dimension_city"
                                    },
                                    "externalReferences": { "connection": "[parameters('datawarehouse')]" }
                                }
                            },
                            "enableStaging": false
                        }
                    }
                ],
                "annotations": []
            },
            "dependsOn": []
        }
    ]
}